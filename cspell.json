{
	"import": ["@cspell/dict-lorem-ipsum/cspell-ext.json"],
	"caseSensitive": false,
	"dictionaries": ["custom-dictionary"],
	"dictionaryDefinitions": [
		{
			"name": "custom-dictionary",
			"path": "./.cspell/custom-dictionary.txt",
			"addWords": true
		}
	],
	"ignorePaths": [
		"**/node_modules/**",
		"**/dist/**",
		"**/build/**",
		"**/lib/**",
		"**/docs/**",
		"**/vendor/**",
		"**/public/**",
		"**/static/**",
		"**/tmp/**",
		"**/out/**",
		"**/*.d.ts",
		"**/package.json",
		"**/*.md",
		"**/stats.html",
		"eslint.config.js",
		".gitignore",
		".prettierignore",
		"cspell.json",
		"commitlint.config.js",
		".cspell"
	]
}
